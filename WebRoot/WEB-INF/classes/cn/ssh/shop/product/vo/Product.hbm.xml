<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    	<class name="cn.ssh.shop.product.vo.Product" table="product">
    		<id name="pid">
    			<generator class="native">
    				<param name="sequence">product_seq</param>
    			</generator>
    		</id>
    		<property name="pname"/>
    		<property name="market_price"/>
    		<property name="shop_price"/>
    		<property name="image"/>
    		<property name="pdesc"/>
    		<property name="is_hot"/>
    		<property name="pdate"/>
    		<many-to-one name="secondCategory" lazy="false" class="cn.ssh.shop.secondcategory.vo.SecondCategory" column="csid"></many-to-one>
    	</class>
    </hibernate-mapping>