<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.ssh.shop.user.vo.User" table="commonuser">
		<id name="userid">
			<generator class="native">
				<param name="sequence">commonuser_seq</param>
			</generator>
		</id>
		<property name="username"/>
		<property name="pass"/>
		<property name="realname"/>
		<property name="email"/>
		<property name="phone"/>
		<property name="addr"/>
		<property name="state"/>
		<property name="code"/>
	</class>
</hibernate-mapping>